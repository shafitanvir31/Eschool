<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Category Rankings</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
      background-color: #f4f4f4;
    }
    .container {
      max-width: 800px;
      margin: 20px auto;
      padding: 20px;
      background-color: #fff;
      border-radius: 5px;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }
    .category {
      background-color: #f0f0f0;
      padding: 10px;
      margin-top: 20px;
      border-radius: 5px;
    }
    .category h2 {
      margin: 0;
      padding-bottom: 10px;
      border-bottom: 1px solid #ccc;
    }
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 10px;
    }
    th, td {
      padding: 10px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
    tr:hover {
      background-color: #f9f9f9;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1>Category Rankings</h1>
    <% Object.entries(categoryRankings).forEach(([category, rankings]) => { %>
      <div class="category">
        <h2><%= category %></h2>
        <table>
          <tr>
            <th>User ID</th>
            <th>Rank</th>
          </tr>
          <% rankings.forEach(rank => { %>
            <tr>
              <td><%= rank.user_id %></td>
              <td><%= rank.rank %></td>
            </tr>
          <% }); %>
        </table>
      </div>
    <% }); %>
  </div>
</body>
</html>
